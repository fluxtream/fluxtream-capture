[
  {
    "do": {
      "android_add_permission": {
        "permission": "android.permission.BLUETOOTH"
      }
    }
  }, {
    "do": {
      "android_add_permission": {
        "permission": "android.permission.BLUETOOTH_ADMIN"
      }
    }
  }, {
    "do": {
      "android_add_to_application_manifest": {
        "element": {
          "tag": "service",
          "attributes": {
            "android:name": "io.trigger.forge.android.modules.flx_polar_h7.PolarH7Service"
          }
        }
      }
    }
  }, {
    "do": {
      "android_add_to_application_manifest": {
        "element": {
          "tag": "activity",
          "attributes": {
            "android:name": "io.trigger.forge.android.modules.flx_polar_h7.EnableBluetoothActivity",
            "android:theme": "@android:style/Theme.Translucent.NoTitleBar"
          }
        }
      }
    }
  }, {
    "do": {
      "android_add_to_manifest": {
        "element": {
          "tag": "uses-feature",
          "attributes": {
            "android:name": "android.hardware.bluetooth_le",
            "android:required": "false"
          }
        }
      }
    }
  }
]
