{
	"config_version": "4",
	"name": "FluxtreamCapture",
	"author": "julien.dupuis@gmail.com",
	"version": "0.1",
	"platform_version": "v2.1.7",
	"description": "An empty app created by default",
	"core": {
		"general": {
			"logging": {
				"level": "DEBUG"
			},
			"reload": true,
			"trusted_urls": [
				"http://fluxtream.dev/*",
				"http://fluxtream.org/*"
			]
		},
		"ios": {
			"minimum_version": "4.3",
			"device_family": "any",
			"package_name": "org.fluxtream.capture.*"
		}
	},
	"modules": {
		"request": {
			"version": "2.5",
			"config": {
				"permissions": [
					"*"
				]
			}
		},
		"file": {
			"version": "2.4"
		},
		"prefs": {
			"version": "2.0"
		},
		"fc_gallery": {
			"version": "0.3"
		},
		"urlhandler": {
			"version": "2.0",
			"config": {
				"scheme": "fluxtream"
			}
		},
		"icons": {
			"version": "2.2",
			"config": {
				"android": {
					"36": "icons/f-36x36.png",
					"48": "icons/f-48x48.png",
					"72": "icons/f-72x72.png",
					"96": "icons/f-96x96.png",
					"144": "icons/f-144x144.png"
				},
				"ios": {
					"57": "icons/f-57x57.png",
					"72": "icons/f-72x72.png",
					"76": "icons/f-76x76.png",
					"114": "icons/f-114x114.png",
					"120": "icons/f-120x120.png",
					"144": "icons/f-144x144.png",
					"152": "icons/f-152x152.png"
				}
			}
		}
	}
}